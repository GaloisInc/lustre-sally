let
  signal : bool
    = And(And(And(Lt(x, y), Lt(y, z)), Lt(z, w)), Lt(x, w))
   
  pre_counter : int
    = pre counter
   
  init_pre_counter : int
    = 0 -> pre_counter
   
  counter : int
    = ITE(signal, Add(1, init_pre_counter), 0)
   
  cex : bool
    = Leq(counter, 10)
