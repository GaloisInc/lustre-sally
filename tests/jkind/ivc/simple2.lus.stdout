(define-state-type
   S
   (
     (a1 Int)
     (a2 Int)
     (ok Bool)
     (x Bool)
     (y Bool)
     (z Bool)
     (|gal-initializing| Bool))
   (
     (a1 Int)
     (a2 Int)))
(define-transition-system
   TS
   S
   (= |gal-initializing| true)
   (and
      (= next.|gal-initializing| false)
      (= next.a1 input.a1)
      (= next.a2 input.a2)
      next.x
      (=
         next.y
         (< 0 next.a1))
      (=
         next.z
         (< 10 next.a2))
      (=
         next.x
         (and next.y next.z))
      (=
         next.ok
         (< 0 next.a1))))

(query
   TS
   (or |gal-initializing| ok))

